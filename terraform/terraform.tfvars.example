# AWS Configuration
aws_region = "us-east-1"

# **REQUIRED**: Provide your existing VPC resources
vpc_id           = "vpc-xxxxxxxxx"        # Your VPC ID
subnet_id        = "subnet-xxxxxx"        # Your Subnet ID  
security_group_id = "sg-xxxxxxxxx"        # Your Security Group ID

# Instance Configuration
instance_count = 5
instance_type  = "t3.micro"

# Optional: Specify a custom AMI (Amazon Linux 2 example shown)
# ami = "ami-0c02fb55956c7d316"

# Environment
environment = "production"
project_name = "ip-allocation"

# Tags
tags = {
  ManagedBy = "Terraform"
  Project   = "ip-allocation"
  Owner     = "DevOps"
}
